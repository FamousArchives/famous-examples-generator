{
  "name": "famous-examples-generator",
  "version": "0.0.0",
  "description": "Generate Templates for Famous Examples",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/famous/famous-examples-generator.git"
  },
  "scripts": {
    "test": "npm run-script lint && tape test/test-*.js | tap-spec",
    "watch": "nodemon --ignore node_modules/ -e js,json --exec 'npm test'",
    "lint": "eslint **/*.js"
  },
  "author": "Myles Borins <myles@famo.us>",
  "devDependencies": {
    "eslint": "^0.8.2",
    "mkdirp": "^0.5.0",
    "nodemon": "^1.2.1",
    "rimraf": "^2.2.8",
    "tap-spec": "^1.0.0",
    "tape": "^3.0.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "findit": "^2.0.0",
    "lodash": "^2.4.1",
    "minimist": "^1.1.0",
    "mkdirp": "^0.5.0"
  },
  "bin": {
    "famous-examples-generator": "bin/cmd.js"
  },
  "preferGlobal": "true",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "readme": "README.markdown",
  "engines": {
    "node": ">=0.10.28",
    "npm": ">=1.4.10"
  },
  "license": "TBD"
}
